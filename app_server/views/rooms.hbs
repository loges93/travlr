{{> header}}
<div id="contents">
    <div class="box">
        <div>
            <div class="body">
                <h1>Rooms</h1>
                <ul id="rooms">
                {{#each room_list}}
                    <li>
                        <a href="/rooms"><img src="images/{{this.image}}" alt="Img"></a>
                        <h2><a href="/rooms">{{this.roomType}}</a></h2>
                        <p>
                            {{this.description}}
                        </p>
                        <span class="rate">Rate: {{this.rate}}</span>
                    </li>
                {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>
{{> footer}}
